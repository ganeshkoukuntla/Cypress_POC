{
  "name": "cypress_poc",
  "version": "1.0.0",
  "description": "contains cypress framework tests",
  "main": "index.js",
  "scripts": {
    "test": "cypress run cypress/e2e/CitiBot/StepDefinations/*/*.cy.js",
    "test:allure": "cypress run cypress/e2e/CitiBot/StepDefinations/*/*.cy.js --browser chrome --headed --env allure=true && allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkoukuntla/Cypress_POC.git"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^2.2.6",
    "@mmisty/cypress-allure-adapter": "^0.10.7",
    "@testing-library/cypress": "^10.0.1",
    "allure-commandline": "^2.24.0",
    "cypress": "^13.3.3",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.6.0",
    "cypress-xpath": "^2.0.1",
    "mocha-allure-reporter": "^1.4.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/CitiBot/StepDefinations/**"
  },
  "keywords": [
    "Cypress"
  ],
  "author": "Ganeshkumar Koukuntla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ganeshkoukuntla/Cypress_POC/issues"
  },
  "homepage": "https://github.com/ganeshkoukuntla/Cypress_POC#readme",
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}
